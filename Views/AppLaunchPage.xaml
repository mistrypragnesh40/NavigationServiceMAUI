<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:viewModels="clr-namespace:NavigationServiceMAUI.ViewModels"
             x:Class="NavigationServiceMAUI.Views.AppLaunchPage"
             x:DataType="viewModels:AppLaunchPageViewModel"
             Title="AppLaunchPage">
    <VerticalStackLayout>
        <Label 
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Button Text="Navigate To STudent LIst" Command="{Binding NavigateToStudentListCommand}" />
    </VerticalStackLayout>
</ContentPage>